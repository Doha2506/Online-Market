<!DOCTYPE html>
<html lang="en">
<head>
     <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>E-Market</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="shortcut icon" type="image/png" href="~/materials/market.png">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link href="~/assets/css/style.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Market.styles.css" asp-append-version="true" />
</head>

    <!-- ======= Header ======= -->
  <header id="header" class="fixed-top">
    <div class="container d-flex align-items-center justify-content-between">

      <img src="~/materials/logo.png" style="width:150px;height:50px;"></img>
      <nav id="navbar" class="navbar">
        <ul>
        
           <li><a class="nav-link scrollto" href="/Admin/productTable">HOME</a></li>

           <li><a class="nav-link scrollto" href="/Admin/AdminSearch">Search</a></li>
           
          <li><a class="nav-link scrollto" href="/Admin/AddminRegister">Add Admin</a></li>

          <li><a class="nav-link scrollto" href="/Admin/Addproduct">Add product</a></li>

          <li><a class="nav-link scrollto" href="/Admin/SalesReport">Sales Report</a></li>

          <li><a class="nav-link scrollto" href="/Admin/ShowFeedback">Show Feedbacks</a></li>

          <li><a class="nav-link scrollto" href="/Admin/ShowComments">Show Comments</a></li>

          <li><a class="nav-link scrollto" href="/Admin/ShowUsers">Show Users</a></li>

           <li><a class="nav-link scrollto" href="/Admin/ShowAdmins">Show Admins</a></li>
        </ul>
        <i class="bi bi-list mobile-nav-toggle"></i>
      </nav><!-- .navbar -->

    </div>
  </header><!-- End Header -->


    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


  <div id="preloader"></div>
  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
  <script src="~/assets/vendor/purecounter/purecounter.js"></script>
  <script src="~/assets/vendor/aos/aos.js"></script>
  <script src="~/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="~/assets/vendor/glightbox/js/glightbox.min.js"></script>
  <script src="~/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
  <script src="~/assets/vendor/swiper/swiper-bundle.min.js"></script>
  <script src="~/assets/vendor/php-email-form/validate.js"></script>

  <!-- Template Main JS File -->
  <script src="~/assets/js/main.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

</html>

